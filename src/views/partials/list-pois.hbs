{{#each poiList}}
  <div class="box box-link-hover-shadow">
    <h2 class="name">
      {{name}}
    </h2>
    <a href="/poi/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open"></i>
      </span>
    </a>
    <a href="/dashboard/deletepoi/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}